{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": ".deut File Format v1.1",
  "type": "object",
  "required": ["meta", "input_dna", "outputs"],
  "properties": {
    "meta": {
      "type": "object",
      "required": ["version", "userId", "fingerprint"],
      "properties": {
        "version": { "type": "string", "const": "1.1" },
        "userId": { "type": "string" },
        "fingerprint": { "type": "string", "pattern": "^[a-f0-9]{64}$" }
      }
    },
    "input_dna": {
      "type": "object",
      "required": ["selectors"],
      "properties": {
        "selectors": { "type": "object" }
      }
    },
    "outputs": {
      "type": "object",
      "required": ["midjourney", "natural", "raw"],
      "properties": {
        "midjourney": { "type": "string" },
        "natural": { "type": "string" },
        "raw": { "type": "string" }
      }
    }
  }
}
